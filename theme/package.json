{
  "name": "gatsby-theme-rocket",
  "description": ":rocket: A super fast blog",
  "version": "1.1.0",
  "author": "Raincal <cyj94228@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Raincal/gatsby-theme-rocket"
  },
  "keywords": [
    "gatsby",
    "gatsby-theme",
    "gatsby-plugin",
    "blog"
  ],
  "license": "MIT",
  "scripts": {
    "format:code": "npm run lint:code -- --fix",
    "lint:code": "eslint --ignore-path .gitignore --ext .js,.jsx .",
    "lint:md": "remark src/pages/",
    "format": "prettier --write 'src/**/*.{js,jsx}'"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.1",
    "@mdx-js/react": "^1.6.1",
    "dayjs": "^1.8.23",
    "gatsby-paginate": "^1.1.1",
    "gatsby-plugin-brotli": "^1.3.1",
    "gatsby-plugin-catch-links": "^2.2.1",
    "gatsby-plugin-feed": "^2.4.1",
    "gatsby-plugin-layout": "^1.2.1",
    "gatsby-plugin-mdx": "^1.1.7",
    "gatsby-plugin-netlify": "^2.2.1",
    "gatsby-plugin-no-sourcemaps": "^2.2.0",
    "gatsby-plugin-page-creator": "^2.2.1",
    "gatsby-plugin-postcss": "^2.2.1",
    "gatsby-plugin-react-helmet": "^3.2.2",
    "gatsby-plugin-sharp": "^2.5.4",
    "gatsby-plugin-sitemap": "^2.3.2",
    "gatsby-remark-autolink-headers": "^2.2.1",
    "gatsby-remark-images": "^3.2.2",
    "gatsby-remark-prismjs": "^3.4.1",
    "gatsby-remark-yuque-images": "^1.2.0",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-remark": "^2.7.1",
    "gatsby-transformer-yaml": "^2.3.1",
    "lodash": "^4.17.15",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prismjs": "^1.20.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "gatsby": "^2.20.20",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "remark-cli": "^8.0.0",
    "remark-preset-lint-recommended": "^4.0.0"
  },
  "peerDependencies": {
    "gatsby": "^2.20.20",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "lint-no-shortcut-reference-link",
        false
      ],
      [
        "lint-no-undefined-references",
        false
      ]
    ]
  }
}
